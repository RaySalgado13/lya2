Simbolo Identifier(String tipoDato, String categoria, int tamanio):{String lexema;}
{
	<IDENTIFICADOR>{
		lexema = token.image; 
		Simbolo s = new Simbolo(lexema, tipoDato, categoria, tamanio);
		if (validate(s)){
			tablaSimbolos.put(s.hashCode(),s);
		}
		
		return s;
	}
